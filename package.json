    "@noble/secp256k1": "^3.0.0",
    "@picocss/pico": "^2.1.1",
    "@simplewebauthn/browser": "^13.2.2",
    "@simplewebauthn/server": "^13.2.2",
    "hono": "^4.11.4",
    "lru-cache": "^11.2.4",
    "multiformats": "^13.4.2",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260114.0",
    "@types/http-proxy": "^1.17.17",
